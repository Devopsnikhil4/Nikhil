pipeline{
    agent any
    environment {
        SSHCRED = credentials('SSH_CRED')
    }
    stages {

        stage('install boto3') {
            steps {
                sh '''            
                    pip3 install boto3
                '''
            }
        }
        
        stage('createEC2') {
            steps {
                sh '''            
                    ansible-playbook createEC2.yaml
                '''
            }
        }
    }
}